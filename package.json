  "stability": "experimental",
  "jsii": {
    "outdir": "dist",
    "targets": {
      "python": {
        "distName": "cdk8s-argocd-resources",
        "module": "cdk8s_argocd_resources"
      },
      "go": {
        "moduleName": "github.com/opencdk8s/cdk8s-argocd-resources-go"
